{
    "author": "Jakobg1215",
    "description": "A Minecraft Java server writen in TypeScript.",
    "devDependencies": {
        "@types/node": "^15.12.2",
        "@types/uuid": "^8.3.0",
        "concurrently": "^6.2.0",
        "import-sort-style-module": "^6.0.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.1",
        "prettier-plugin-import-sort": "^0.0.7"
    },
    "dependencies": {
        "uuid": "^8.3.2"
    },
    "files": [
        "./dist/*"
    ],
    "importSort": {
        ".ts": {
            "style": "module",
            "parser": "typescript"
        }
    },
    "main": "./dist/server.js",
    "name": "barrierts",
    "private": true,
    "scripts": {
        "build": "tsc",
        "dev": "concurrently -k \"tsc -w\" \"nodemon ./dist/server.js\"",
        "prettier": "prettier ./src --config ./.prettierrc --write",
        "start": "node ./dist/server.js --unhandled-rejections=strict"
    },
    "version": "1.0.0"
}
