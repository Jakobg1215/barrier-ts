{
    "author": "Jakobg1215",
    "description": "A Minecraft Java server writen in TypeScript.",
    "devDependencies": {
        "@types/node": "^16.10.3",
        "concurrently": "^6.3.0",
        "nodemon": "^2.0.13",
        "prettier": "^2.4.1",
        "typescript": "^4.4.3"
    },
    "engines": {
        "node": ">=10"
    },
    "license": "MIT",
    "main": "dist/server",
    "name": "barrierts",
    "private": true,
    "scripts": {
        "build": "tsc",
        "dev:tsc": "tsc -w",
        "dev:nodemon": "nodemon dist/server --delay 1",
        "dev": "concurrently -k -i -s \"all\" npm:dev:*",
        "start": "node dist/server",
        "prettier": "prettier src --config .prettierrc --write"
    },
    "version": "1.0.0"
}
