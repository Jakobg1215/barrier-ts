{
    "name": "barrierts",
    "engines": {
        "node": ">=17.7.0",
        "npm": ">=8.5.0"
    },
    "devDependencies": {
        "@types/node": "^17.0.21",
        "concurrently": "^7.0.0",
        "nodemon": "^2.0.15",
        "typescript": "^4.6.2"
    },
    "scripts": {
        "start": "node dist/index.js",
        "dev": "concurrently -i --default-input-target dev:watch -r npm:dev:*",
        "dev:watch": "nodemon --signal SIGHUP dist/index",
        "dev:build": "tsc -w",
        "build": "tsc"
    }
}
